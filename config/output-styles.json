    "cssr_fail": {
      "color": "red",
      "bold": true,
      "italic": false,
      "underline": false,
      "prefix": "[CSSR:‚úó]",
      "backgroundColor": null,
      "description": "CSSR safety checks failed"
    },

    "_comment_system": "=== SYSTEM OUTPUTS ===",

    "system_startup": {
      "color": "green",
      "bold": true,
      "italic": false,
      "underline": false,
      "prefix": "‚ö°",
      "backgroundColor": null,
      "description": "System initialization and startup messages"
    },

    "system_shutdown": {
      "color": "yellow",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚èπ",
      "backgroundColor": null,
      "description": "System shutdown and cleanup messages"
    },

    "system_config": {
      "color": "blue",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚öô",
      "backgroundColor": null,
      "description": "Configuration load/save and environment setup"
    },

    "system_env": {
      "color": "gray",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "[ENV]",
      "backgroundColor": null,
      "description": "Environment variable loading and configuration"
    }
  },

  "colorPalette": {
    "_description": "ANSI 16-color palette reference for terminal output",
    "standard": {
      "black": "\u001b[30m",
      "red": "\u001b[31m",
      "green": "\u001b[32m",
      "yellow": "\u001b[33m",
      "blue": "\u001b[34m",
      "magenta": "\u001b[35m",
      "cyan": "\u001b[36m",
      "white": "\u001b[37m",
      "gray": "\u001b[90m"
    },
    "bright": {
      "brightBlack": "\u001b[90m",
      "brightRed": "\u001b[91m",
      "brightGreen": "\u001b[92m",
      "brightYellow": "\u001b[93m",
      "brightBlue": "\u001b[94m",
      "brightMagenta": "\u001b[95m",
      "brightCyan": "\u001b[96m",
      "brightWhite": "\u001b[97m"
    },
    "formatting": {
      "reset": "\u001b[0m",
      "bold": "\u001b[1m",
      "dim": "\u001b[2m",
      "italic": "\u001b[3m",
      "underline": "\u001b[4m"
    },
    "background": {
      "bgBlack": "\u001b[40m",
      "bgRed": "\u001b[41m",
      "bgGreen": "\u001b[42m",
      "bgYellow": "\u001b[43m",
      "bgBlue": "\u001b[44m",
      "bgMagenta": "\u001b[45m",
      "bgCyan": "\u001b[46m",
      "bgWhite": "\u001b[47m"
    }
  },

  "extensibility": {
    "allowCustomStyles": true,
    "fallbackToDefault": true,
    "userStylesPath": "config/output-styles.user.json",
    "description": "Custom user styles can override defaults by creating output-styles.user.json"
  }
}
