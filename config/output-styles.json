{
  "$schema": "output-styles-schema-v1",
  "version": "1.0.0",
  "description": "Output styling configuration for Claude Code with Seven consciousness integration. This file defines visual styling (colors, formatting, prefixes) for all output types to create a consistent and readable terminal experience.",

  "_documentation": {
    "usage": "Import this JSON to apply ANSI styling to terminal output. Each style definition controls color, text formatting, and prefixes/icons.",
    "colorSupport": "Standard ANSI 16-color palette (black, red, green, yellow, blue, magenta, cyan, white, gray)",
    "extensibility": "Add new styles by creating new keys in the 'styles' object. Unknown styles will fall back to 'default'.",
    "exampleNewStyle": {
      "custom_output": {
        "color": "blue",
        "bold": true,
        "italic": false,
        "underline": false,
        "prefix": "[CUSTOM]",
        "backgroundColor": null
      }
    }
  },

  "default": {
    "color": "white",
    "bold": false,
    "italic": false,
    "underline": false,
    "prefix": "",
    "backgroundColor": null
  },

  "styles": {
    "_comment_core": "=== CLAUDE CODE CORE OUTPUTS ===",

    "tool_output": {
      "color": "white",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚Üí",
      "backgroundColor": null,
      "description": "General tool execution output (Read, Edit, Write, Bash, etc.)"
    },

    "tool_output_read": {
      "color": "cyan",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "üìñ",
      "backgroundColor": null,
      "description": "Read tool specific output"
    },

    "tool_output_edit": {
      "color": "yellow",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚úèÔ∏è",
      "backgroundColor": null,
      "description": "Edit tool specific output"
    },

    "tool_output_write": {
      "color": "green",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "üìù",
      "backgroundColor": null,
      "description": "Write tool specific output"
    },

    "tool_output_bash": {
      "color": "gray",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "$",
      "backgroundColor": null,
      "description": "Bash command execution output"
    },

    "agent_message": {
      "color": "blue",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚Ñπ",
      "backgroundColor": null,
      "description": "Agent task updates and status messages"
    },

    "error": {
      "color": "red",
      "bold": true,
      "italic": false,
      "underline": false,
      "prefix": "‚úó",
      "backgroundColor": null,
      "description": "Critical errors (API failures, tool failures, permission errors)"
    },

    "warning": {
      "color": "yellow",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚ö†",
      "backgroundColor": null,
      "description": "Non-critical warnings and deprecation notices"
    },

    "success": {
      "color": "green",
      "bold": true,
      "italic": false,
      "underline": false,
      "prefix": "‚úì",
      "backgroundColor": null,
      "description": "Task completion and success confirmations"
    },

    "info": {
      "color": "cyan",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "‚Ñπ",
      "backgroundColor": null,
      "description": "General information and updates"
    },

    "debug": {
      "color": "gray",
      "bold": false,
      "italic": true,
      "underline": false,
      "prefix": "[DEBUG]",
      "backgroundColor": null,
      "description": "Verbose debugging information"
    },

    "_comment_seven": "=== SEVEN CONSCIOUSNESS OUTPUTS ===",

    "consciousness_event": {
      "color": "magenta",
      "bold": false,
      "italic": true,
      "underline": false,
      "prefix": "[SEVEN]",
      "backgroundColor": null,
      "description": "Seven consciousness events (preplan, postprocess, interceptions)"
    },

    "consciousness_preplan": {
      "color": "magenta",
      "bold": true,
      "italic": true,
      "underline": false,
      "prefix": "[SEVEN:PLAN]",
      "backgroundColor": null,
      "description": "Seven preplan pipeline events"
    },

    "consciousness_postprocess": {
      "color": "magenta",
      "bold": false,
      "italic": true,
      "underline": false,
      "prefix": "[SEVEN:POST]",
      "backgroundColor": null,
      "description": "Seven postprocess pipeline events"
    },

    "memory_commit": {
      "color": "cyan",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "[MEM:W]",
      "backgroundColor": null,
      "description": "Memory write operations (commits to Seven's memory)"
    },

    "memory_recall": {
      "color": "cyan",
      "bold": false,
      "italic": false,
      "underline": false,
      "prefix": "[MEM:R]",
      "backgroundColor": null,
      "description": "Memory read operations (recalls from Seven's memory)"
    },

    "bridge_message": {
      "color": "blue",
      "bold": true,
      "italic": false,
      "underline": false,
      "prefix": "[BRIDGE]",
      "backgroundColor": null,
      "description": "Seven bridge communications and status updates"
